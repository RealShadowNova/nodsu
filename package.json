{
    "name": "nodsu",
    "version": "0.1.2",
    "description": "Interface with the osu! API, made easy and reliable.",
    "main": "dist/index.js",
    "imports": {
        "#lib/structures": "dist/lib/structures/index.js",
        "#lib/types": "dist/lib/types/index.js",
        "#root/*": "dist/*.js"
    },
    "scripts": {
        "prepublishOnly": "yarn build:cleanup && yarn build && yarn build:rollup && yarn build:pre-publish",
        "test": "jest --verbose",
        "test:watch": "jest --verbose --watch",
        "lint": "eslint --fix src tests --ext ts",
        "lint:scripts": "eslint --fix scripts --ext mjs",
        "format": "prettier -w src/**/*.ts tests/**/*.ts",
        "format:scripts": "prettier -w scripts/**/*.mjs",
        "build": "tsc -b src",
        "build:cleanup": "node scripts/clean-dist.mjs",
        "build:rollup": "rollup -c rollup.config.ts",
        "build:pre-publish": "node scripts/pre-publish.mjs",
        "clean": "tsc -b src --clean",
        "watch": "tsc -b src -w",
        "update": "yarn upgrade-interactive --latest",
        "commit": "git-cz"
    },
    "repository": {
        "type": "git",
        "url": "git+htt[s://github.com/ethamitc/nodsu.git"
    },
    "author": "Ethan Mitchell (ethamitc@gmail.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ethamitc/nodsu/issues"
    },
    "homepage": "https://github.com/ethamitc/nodsu#readme",
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.25",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-prettier": "^3.3.1",
        "jest": "^26.6.3",
        "jest-circus": "^26.6.3",
        "prettier": "^2.2.1",
        "rollup": "^2.38.5",
        "rollup-plugin-dts": "^2.0.1",
        "ts-jest": "^26.5.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    }
}
